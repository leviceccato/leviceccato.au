---
import * as t from '../tookit'
import getSmartUrl from './_logos/get-smart.svg'
import brookesUrl from './_logos/brookes.svg'
import rxEliteUrl from './_logos/rx-elite.svg'
import arcUrl from './_logos/arc.svg'
import rubixUrl from './_logos/rubix.svg'
import addMyLogoUrl from './_logos/add-my-logo.svg'
import diceRollerUrl from './_logos/dice-roller.svg'
import cityOfNewcastleUrl from './_logos/city-of-newcastle.svg'
import newcastleFringeUrl from './_logos/newcastle-fringe.svg'
import walkOfTheWanderingManUrl from './_logos/walk-of-the-wandering-man.svg'
import arcadiaUrl from './_logos/arcadia.svg'
import designWithDevUrl from './_logos/design-with-dev.svg'
---

<t.Layout
	title="Logos"
	description="A collection of logos and marks for various commerical and non-commercial projects."
>
	<t.H1>Logos</t.H1>
	<t.P>
		A collection of logos and marks for various commerical and non-commercial
		projects.
	</t.P>
	<t.Grid columns="2">
		<t.Logo
			src={getSmartUrl.src}
			alt=""
			scale="0.55"
		/>
		<t.Logo
			src={brookesUrl.src}
			alt=""
			scale="0.55"
		/>
		<t.Logo
			src={rxEliteUrl.src}
			alt=""
			scale="0.47"
		/>
		<t.Logo
			src={arcUrl.src}
			alt=""
			scale="0.58"
		/>
		<t.Logo
			src={rubixUrl.src}
			alt=""
			scale="0.48"
		/>
		<t.Logo
			src={addMyLogoUrl.src}
			alt=""
			scale="0.48"
		/>
		<t.Logo
			src={diceRollerUrl.src}
			alt=""
			scale="0.65"
		/>
		<t.Logo
			src={cityOfNewcastleUrl.src}
			alt=""
			scale="0.65"
		/>
		<t.Logo
			src={newcastleFringeUrl.src}
			alt=""
			scale="0.64"
		/>
		<t.Logo
			src={walkOfTheWanderingManUrl.src}
			alt=""
			scale="0.71"
		/>
		<t.Logo
			src={arcadiaUrl.src}
			alt=""
			scale="0.54"
		/>
		<t.Logo
			src={designWithDevUrl.src}
			alt=""
			scale="0.64"
		/>
	</t.Grid>
</t.Layout>
